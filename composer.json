{
    "name": "zaver/woocommerce-gateway",
    "type": "project",
    "config": {
        "platform": {
            "php": "7.4.0"
        },
        "allow-plugins": {
            "wpify/scoper": true
        }
    },
    "require-dev": {
        "wpify/scoper": "3.1.3",
        "php-stubs/woocommerce-stubs": "*"
    },
    "extra": {
        "wpify-scoper": {
            "prefix": "KrokedilZCODeps",
            "folder": "dependencies",
            "autorun": true,
            "globals": [
                "wordpress",
                "woocommerce"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Krokedil\\Zaver\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
